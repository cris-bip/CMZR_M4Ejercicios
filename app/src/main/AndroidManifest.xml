<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.M4Ejercicios"
        tools:targetApi="31">
        <activity
            android:name=".passingparameters.FirstActivity"
            android:exported="false" />
        <activity
            android:name=".implicitintent.IntentImplicitoActivity"
            android:exported="false" />
        <activity
            android:name=".lifecycle.LifeCycleActivity"
            android:exported="false" />
        <activity
            android:name=".componentesgraficos.FrameActivity"
            android:exported="true">

            <!-- Filtro para ejecutar la actividad como actividad principal -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".passingparameters.SecondActivity"
            android:exported="false" />

        <!--<activity
            android:name=".passingparameters.SecondActivity"
            android:exported="false"
            android:parentActivityName=".passingparameters.FirstActivity"
            />-->
    </application>

</manifest>